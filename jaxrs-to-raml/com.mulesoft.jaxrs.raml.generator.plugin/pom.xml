   <build>      
	<sourceDirectory>../com.mulesoft.jaxrs.raml.generator/src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        	       
        </configuration>
      </plugin>
